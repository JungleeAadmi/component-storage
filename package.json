{
  "name": "component-storage",
  "version": "1.0.0",
  "description": "Self-hosted electronic component inventory management system",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && nodemon server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "NODE_ENV=production node backend/server.js",
    "migrate": "node backend/migrations/migrate.js",
    "backup": "node backend/services/backupService.js",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": ["component", "inventory", "storage", "electronics", "self-hosted"],
  "author": "JungleeAadmi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JungleeAadmi/component-storage.git"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
